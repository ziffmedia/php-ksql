{
    "name": "ziffmedia/ksql",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ZiffMedia\\Ksql\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Josh Butts",
            "email": "josh.butts@ziffmedia.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^1.22",
        "symfony/var-dumper": "^6.2",
        "laravel/pint": "^1.4",
        "orchestra/testbench": "^7.19",
        "calebporzio/sushi": "^2.4",
        "pestphp/pest-plugin-laravel": "^1.4",
        "mockery/mockery": "^1.5"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
